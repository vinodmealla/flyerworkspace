<script>

var url=window.location.href;
var imgurl=url.split("?");
var img1=imgurl[1].split("=");
var img=img1[1];
var ext=img.split(".");

document.getElementById('imagez1').src ="images/"+img;
document.getElementById('imagez2').src ="images/"+ext[0]+"_01."+ext[1];

